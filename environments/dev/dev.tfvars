# dev.tfvars

# AWS region for the development environment
aws_region = "eu-west-2"

# EC2 instance type for the development environment
instance_type = "t2.micro"

# Number of EC2 instances to create
instance_count = 1

# RDS database engine for the development environment
db_engine = "mysql"

# RDS database instance class
db_instance_class = "db.t2.micro"

# RDS allocated storage (in GB)
db_allocated_storage = 20


